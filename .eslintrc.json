{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "plugin:cypress/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "cypress"
    ],
    "rules": {
        "vue/no-multiple-template-root": "off"
    }
}
